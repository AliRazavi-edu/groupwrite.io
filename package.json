{
  "name": "groupwrite.io",
  "version": "0.1.0",
  "description": "A realtime collaborative writing game Edit",
  "author": "Ron Gross <chiefninjaofficer@gmail.com>",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/cross-env mode=dev ./node_modules/.bin/nodemon build/server.js",
    "prestartprod": "npm run decrypt ${enc_password}",
    "startprod": "./node_modules/.bin/cross-env mode=prod node build/server.js",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "mocha test/e2e -u bdd -R spec",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "babel-node": "babel-node --presets=es2016,es2015",
    "encrypt": "encrypt config/prod.secret.config.js config/prod.secret.config.js.enc",
    "decrypt": "decrypt config/prod.secret.config.js.enc config/prod.secret.config.js"
  },
